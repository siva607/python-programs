def power_1(A, B):   
    if B == 0:  
        return 1  
    if B % 2 == 0:  
        return power_1(A, B // 2) * power(A, B // 2)         
    return A * power(A, B // 2) * power(A, B // 2)   
def order_1(A):   
    N = 0  
    while (A != 0):  
        N = N + 1  
        A = A // 10   
    return N  
    
